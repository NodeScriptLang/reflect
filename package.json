{
    "name": "@nodescript/reflect",
    "version": "1.0.1",
    "description": "Reflection Utilities",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": "./out/main/index.js"
    },
    "main": "out/main/index.js",
    "types": "out/main/index.d.ts",
    "files": [
        "out/main/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "tsc -b -w",
        "compile": "tsc -b",
        "lint": "eslint --ext=.js,.ts,.vue --cache .",
        "test": "NODE_ENV=test mocha",
        "version": "npm run compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/nodescript/reflect.git"
    },
    "keywords": [],
    "author": "Boris Okunskiy",
    "license": "ISC",
    "devDependencies": {
        "@nodescript/eslint-config": "^1.0.0",
        "@types/mocha": "^8.2.3",
        "@types/node": "^16.3.1",
        "eslint": "^8.24.0",
        "mocha": "^9.0.2",
        "pre-commit": "^1.2.2",
        "typescript": "^4.3.5"
    },
    "peerDependencies": {
        "reflect-metadata": "^0.1.13"
    }
}
